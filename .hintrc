{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off",
        "image-alt": "off"
      }
    ],
    "meta-viewport": "off",
    "button-type": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "width: fit-content"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "no-inline-styles": "off"
  }
}